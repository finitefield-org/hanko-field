{
  "packages": [
    "go@1.25",
    "flutter@latest",
    "air@latest",
    "curl@latest",
    "rustc@1.92.0",
    "cargo@1.92.0"
  ],
  "env": {
    "GOPATH": "$DEVBOX_PROJECT_ROOT/.go",
    "CARGO_HOME": "$DEVBOX_PROJECT_ROOT/.devbox/cargo",
    "PATH": "$DEVBOX_PROJECT_ROOT/.devbox/bin:$GOPATH/bin:$PATH"
  },
  "shell": {
    "init_hook": [
      "echo 'Hanko Field devbox ready (Go/Flutter/Firebase/Ironframe).'"
    ],
    "scripts": {
      "ironframe": [
        "./scripts/ironframe.sh"
      ]
    }
  }
}
